    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "puppeteer": "1.8.0",
    "workbox-build": "3.4.1",
    "jest-html-reporter": "^2.4.2",    
    "jest-junit": "^5.2.0"
  }
