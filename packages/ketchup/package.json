{
  "name": "ketchup",
  "version": "0.0.1",
  "keywords": [
    "Sme.UP",
    "KetchUP",
    "webcomponents"
  ],
  "description": "Sme.UP webcomponents library",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/ionic.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
