    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/sass": "^0.1.1"
  },
  "devDependencies": {
    "@stencil/core": "~0.18.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "puppeteer": "^1.14.0"
  },
  "license": "MIT"
}
